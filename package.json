{
  "name": "@aiden-ai/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript SDK for the Aiden AI API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aiden",
    "ai",
    "sdk",
    "api",
    "chat",
    "rag",
    "knowledge",
    "llm",
    "typescript"
  ],
  "author": "Aiden Tech GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aiden-ai/aiden-sdk.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0"
  }
}
